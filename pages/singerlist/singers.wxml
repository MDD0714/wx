<!--pages/singerlist/singers.wxml-->
<view>
  <text>歌手列表</text>
  <view class="singerlist" >
    <view class="item" wx:for="{{singerlist}}" wx:key="singerid" bindtap="getSong" data-singerName="{{item.singername}}">
      <image src="{{item.imgurl}}"></image>
      <text>{{item.singername}}</text>
    </view>
  </view>
</view>
