/* pages/music-ranking/rank.wxss */
.header {
  height: 40px;
}

.rankList {
  width: 100%;
  padding: 5px 10px 0;
  box-sizing: border-box;
}
.cateList {
  width: 100%;
  padding: 5px 10px 0;
  box-sizing: border-box;
}
.item {
  height: 100px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 5px 0;
  background-color: #F7F7F7;
}
.item>image{
  display: block;
  width: 50%;
  height: 100%;
}
.item>text{
  width: 50%;
  height: 100%;
  line-height: 100px;
  text-align: center;
  font-size: 16px;
}